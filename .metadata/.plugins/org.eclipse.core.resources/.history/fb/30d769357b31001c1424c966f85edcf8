import java.util.ArrayList;

public class Array {

	public static void main(String[] args) {
		ArrayList<Integer> arrayList = new ArrayList<>();
		
		arrayList.add(1);
		arrayList.add(2);
		arrayList.add(3);
		arrayList.add(4);
		arrayList.add(5);
		
		ArrayList<Integer> list = arrayList.subList(1,3);
		
		for(int i : arrayList) {
			System.out.println("arratList : " + i);
		}
		
		System.out.println("------------------------");
		
		for(int i = 0; i < list.size(); i++) {
			System.out.println("sublist 메서드로 나겨온 list" + list.get(i));
		}
	}
}
