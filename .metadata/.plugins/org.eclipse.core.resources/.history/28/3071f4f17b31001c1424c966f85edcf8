import java.util.ArrayList;

public class Array {

	public static void main(String[] args) {
		ArrayList<Integer> arrayList = new ArrayList<Integer>(5);
		
		Scanner scan = new Scanner(System.in);
		
		arrayList.add(2);
		arrayList.add(4);
		arrayList.add(6);
		arrayList.add(8);
		arrayList.add(10);
		
		for(int i : arrayList) {
			System.out.println(i);
		}

		
	}
}
