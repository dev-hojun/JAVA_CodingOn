import java.util.Vector;

public class Vector {

	public static void main(String[] args) {
		Vector<Integer> vector = new Vector<Integer>(); // Vector : 동기화 처리에 효율적, ArrayList : 비동기화 처리에 효율적
		
		vector.add(1); // add : 요소를 추가하는 메서드
		vector.addElement(2); // addElement : 별도의 리턴값이 없음
		
		System.out.println(vector);
	}

}
